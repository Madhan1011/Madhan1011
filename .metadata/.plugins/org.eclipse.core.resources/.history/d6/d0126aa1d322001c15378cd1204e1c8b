		System.out.println(a);
		
	//	int b = 18 ;
		
		if (a<=18) {
			System.out.println("Enter the eligible");
			
			} else if(a>12){
				System.out.println("not eligible");
				
		}else {
			System.out.println("invalid vote");
		}
		
		
	}
			
		
		
		
		
		
		
		
		

